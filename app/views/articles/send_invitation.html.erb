
<h1>Articles for you Applied</h1>
<table class="table table-hover">
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Recruiting</th>
      <th>Skills</th>
      <th>Employability</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @agreements.each do |article| %>
        <tr>
          <td><%= article.title %></td>
          <td><%= article.description %></td>
          <td><%= article.recruiting %></td>
          <td><%= article.skills %></td>
          <td><%= article.employability %></td>
          <td><%= link_to 'Show', article %></td>
          <td><%= link_to 'Cancel',cancel_agreement_article_path(article.id) %></td>

        </tr>
    <% end %>
  </tbody>
</table>
